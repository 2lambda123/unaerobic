<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.kovalenych"
          android:versionCode="19"
          android:versionName="1.26">
    <supports-screens android:resizeable="true"
                      android:smallScreens="true"
                      android:normalScreens="true"
                      android:largeScreens="true"
                      android:anyDensity="true"/>

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.VIBRATE"/>
    <uses-permission android:name="android.permission.WAKE_LOCK"/>


    <uses-sdk android:minSdkVersion="8" android:targetSdkVersion="8"/>
    <application android:label="Unaerobic" android:icon="@drawable/icon"
                 android:theme="@android:style/Theme.NoTitleBar"
                 android:name=".UnaeroApplication">
        <activity android:name=".MenuActivity"
                  android:label="@string/app_name"
                  android:screenOrientation="portrait"
                  android:theme="@android:style/Theme.Black.NoTitleBar.Fullscreen"
                >
            <intent-filter>
            <action android:name="android.intent.action.MAIN"/>
            <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".tables.TablesActivity" android:screenOrientation="portrait"/>
        <activity android:name=".tables.CyclesActivity" android:screenOrientation="portrait"/>
        <activity android:name=".tables.ClockActivity" android:screenOrientation="landscape"
                  android:configChanges="orientation"/>
        <activity android:name=".ArticlesActivity" android:screenOrientation="portrait"/>
        <activity android:name=".NostraVideoActivity" android:screenOrientation="portrait"/>
        <activity android:name=".ranking.RankingActivity" android:screenOrientation="portrait"/>
    </application>
</manifest> 
